		<rdf:type rdf:resource="#HydroPlantStorageKind"/>
		<rdfs:label>pumpedStorage</rdfs:label>
		<rdfs:comment>Pumped storage.</rdfs:comment>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPlantStorageKind.runOfRiver">
		<rdf:type rdf:resource="#HydroPlantStorageKind"/>
		<rdfs:label>runOfRiver</rdfs:label>
		<rdfs:comment>Run of river.</rdfs:comment>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPlantStorageKind.storage">
		<rdf:type rdf:resource="#HydroPlantStorageKind"/>
		<rdfs:label>storage</rdfs:label>
		<rdfs:comment>Storage.</rdfs:comment>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPowerPlant">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
		<rdfs:label>#HydroPowerPlant</rdfs:label>
		<rdfs:comment>A hydro power station which can generate or pump. When generating, the generator turbines receive water from an upper reservoir. When pumping, the pumps receive their water from a lower reservoir.</rdfs:comment>
		<cims:belongsToCategory rdf:resource="#Package_Production"/>
		<rdfs:subClassOf rdf:resource="#PowerSystemResource"/>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPowerPlant.HydroGeneratingUnits">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>HydroGeneratingUnits</rdfs:label>
		<rdfs:comment>The hydro generating unit belongs to a hydro power plant.</rdfs:comment>
		<rdfs:domain rdf:resource="#HydroPowerPlant"/>
		<rdfs:range rdf:resource="#HydroGeneratingUnit"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..n"/>
		<cims:inverseRoleName rdf:resource="#HydroGeneratingUnit.HydroPowerPlant"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#aggregateOf"/>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPowerPlant.genRatedP">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>genRatedP</rdfs:label>
		<rdfs:comment>The hydro plant's generating rating active power for rated head conditions.
The attribute shall be a positive value.</rdfs:comment>
		<rdfs:domain rdf:resource="#HydroPowerPlant"/>
		<cims:dataType rdf:resource="#ActivePower"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroPowerPlant.hydroPlantStorageType">
		<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
		<rdfs:label>hydroPlantStorageType</rdfs:label>
		<rdfs:comment>The type of hydro power plant water storage.</rdfs:comment>
		<rdfs:domain rdf:resource="#HydroPowerPlant"/>
		<rdfs:range rdf:resource="#HydroPlantStorageKind"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
