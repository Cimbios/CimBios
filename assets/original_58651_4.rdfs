		<rdfs:range rdf:resource="#SynchronousMachine"/>
		<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:1..n"/>
		<cims:inverseRoleName rdf:resource="#SynchronousMachine.ReactiveCapabilityCurves"/>
	</rdf:Description>
	<rdf:Description rdf:about="#ReactivePower">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
