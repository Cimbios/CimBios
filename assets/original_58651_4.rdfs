		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#attribute"/>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroTurbineKind">
		<rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
		<rdfs:label>#HydroTurbineKind</rdfs:label>
		<rdfs:comment>Type of turbine.</rdfs:comment>
		<cims:belongsToCategory rdf:resource="#Package_GenerationTrainingSimulation"/>
		<cims:stereotype rdf:resource="http://langdale.com.au/2005/UML#enumeration"/>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroTurbineKind.francis">
		<rdf:type rdf:resource="#HydroTurbineKind"/>
		<rdfs:label>francis</rdfs:label>
		<rdfs:comment>Francis.</rdfs:comment>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroTurbineKind.kaplan">
		<rdf:type rdf:resource="#HydroTurbineKind"/>
		<rdfs:label>kaplan</rdfs:label>
		<rdfs:comment>Kaplan.</rdfs:comment>
	</rdf:Description>
	<rdf:Description rdf:about="#HydroTurbineKind.pelton">
		<rdf:type rdf:resource="#HydroTurbineKind"/>
